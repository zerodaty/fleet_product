<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Producto 1: Aceite de Motor -->
        <record id="product_aceite_motor_template" model="product.template">
            <field name="name">Aceite de Motor</field>
            <field name="type">consu</field> <!-- Consumible -->
            <field name="categ_id" ref="product_category_automotriz"/>
            <field name="list_price">100</field> 
            <field name="standard_price">50</field> 
            <field name="sale_line_warn">no-message</field>
            <field name="image_1920" type="base64" file="fleet_product/static/images/aceite.png"/>
        </record>

        <!-- Producto 2: Filtro de Aire -->
        <record id="product_filtro_aire_template" model="product.template">
            <field name="name">Filtro de Aire</field>
            <field name="type">consu</field>
            <field name="categ_id" eval="product_category_automotriz"/> 
            <field name="list_price">150</field> 
            <field name="standard_price">70</field> 
            <field name="sale_line_warn">no-message</field> 
            <field name="image_1920" type="base64" file= "fleet_product/static/images/filtro.jpeg"/>
        </record> 

